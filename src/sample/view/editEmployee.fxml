<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #b6f542;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.editController">
    <children>
        <AnchorPane layoutY="58.0" prefHeight="343.0" prefWidth="700.0" style="-fx-background-color: #ffffff;">
            <children>
                <TextField fx:id="firstname" layoutX="78.0" layoutY="28.0" prefHeight="25.0" prefWidth="198.0" promptText="first name" />
                <TextField fx:id="lastname" layoutX="425.0" layoutY="28.0" prefHeight="25.0" prefWidth="198.0" promptText="last name" />
                <TextField fx:id="username" layoutX="78.0" layoutY="69.0" prefHeight="25.0" prefWidth="198.0" promptText="username" />
                <PasswordField fx:id="password" layoutX="425.0" layoutY="69.0" prefHeight="25.0" prefWidth="198.0" promptText="password" />
                <Button fx:id="employeecreatnew" contentDisplay="CENTER" layoutX="78.0" layoutY="147.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Upate employee" />
                <MenuButton fx:id="jobtitle" alignment="CENTER" layoutX="152.0" layoutY="103.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" text="select">
                    <items>
                        <MenuItem fx:id="adminjob" mnemonicParsing="false" text="admin" />
                        <MenuItem fx:id="employeejob" mnemonicParsing="false" text="employee" />
                    </items>
                </MenuButton>
                <Label layoutX="78.0" layoutY="102.0" prefHeight="25.0" prefWidth="75.0" text="job title:">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
            <Button fx:id="gohome" contentDisplay="CENTER" layoutX="425.0" layoutY="147.0" mnemonicParsing="false" onAction="#goHme" prefHeight="25.0" prefWidth="149.0" text="Back to Employees" />
            </children>
        </AnchorPane>
        <Label fx:id="header" contentDisplay="CENTER" layoutX="217.0" layoutY="14.0" text="Update employee">
            <font>
                <Font name="Arial Black" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
